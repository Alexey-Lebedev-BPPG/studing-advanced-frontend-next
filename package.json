{
  "name": "studying-advanced-frontend-next",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "start:dev:server": "node ./json-server/index.js",
    "start:next": "next dev",
    "start:next:turbo": "next dev --turbo",
    "start:dev": "concurrently \"pnpm start\" \"pnpm run start:dev:server\"",
    "start:dev:next": "concurrently \"pnpm run start:next\" \"pnpm run start:dev:server\"",
    "start:dev:next:turbo": "concurrently \"pnpm run start:next:turbo\" \"pnpm run start:dev:server\"",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "build:prod:next": "next build",
    "lint:ts": "next lint",
    "lint:ts:fix": "next lint --fix",
    "prettier": "prettier --write **/*.{ts,tsx,json}",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./configs/jest/jest.config.ts",
    "test:unit:report": "jest --config ./configs/jest/jest.config.ts --json --outputFile=./reports/unit/.jest-test-results.json",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "loki update --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node ./scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "pnpm run test:ui:json && pnpm run test:ui:html",
    "test:depend": "npx depcruise src --include-only '^src' --config --output-type dot | dot -T svg > reports/dependency-graph/dependency-graph.svg",
    "storybook": "storybook dev -p 6006 -c ./configs/storybook",
    "storybook:debug": "storybook dev -p 6006 -c ./configs/storybook --debug-webpack",
    "storybook:test": "test-storybook -c ./configs/storybook",
    "storybook:build": "storybook build -c ./configs/storybook",
    "prepare": "husky install",
    "generate:slices": "node ./scripts/createSlice/index.js",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node ./scripts/remove-cache.js",
    "remove-features": "npx ts-node ./scripts/remove-feature.ts"
  },
  "overrides": {
    "chrome-remote-interface": "0.31.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.16",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/addon-actions": "^7.2.0",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-designs": "^7.0.1",
    "@storybook/addon-essentials": "^7.2.0",
    "@storybook/addon-interactions": "^7.2.0",
    "@storybook/addon-jest": "^7.2.0",
    "@storybook/addon-links": "^7.2.0",
    "@storybook/addon-mdx-gfm": "^7.2.0",
    "@storybook/addon-storyshots": "^7.2.0",
    "@storybook/addon-storyshots-puppeteer": "^7.2.0",
    "@storybook/addon-storysource": "^7.2.1",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.2.0",
    "@storybook/react-webpack5": "^7.2.0",
    "@storybook/test-runner": "^0.12.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@use-gesture/react": "^10.2.27",
    "concurrently": "^8.2.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-path-checher-ulbi-example": "^0.0.9",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-html-reporters": "^3.1.4",
    "json-server": "^0.17.3",
    "lint-staged": "^14.0.1",
    "loki": "^0.32.0",
    "next-auth": "^4.23.1",
    "next-i18next": "^14.0.0",
    "prettier": "^3.0.3",
    "react-i18next": "^13.2.1",
    "react-redux": "^8.1.2",
    "sass": "^1.66.1",
    "storybook": "^7.2.0",
    "storybook-addon-mock": "^4.1.0",
    "storybook-addon-paddings": "^5.0.0",
    "storybook-addon-pseudo-states": "^2.1.0",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^15.10.2",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
